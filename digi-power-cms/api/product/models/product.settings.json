{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "product",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "description": {
      "type": "text"
    },
    "price": {
      "type": "integer",
      "required": true
    },
    "company": {
      "type": "string"
    },
    "category": {
      "model": "category",
      "via": "product"
    }
  }
}
